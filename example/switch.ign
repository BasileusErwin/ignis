import { println } from "std:io";

let name: string = "John";

match name {
	"John" -> println("Hi John!"),
	"Mary" -> println("Hi Mary!"),
	_ -> println("Hi!"),
}

switch name {
	case "John":
		println("Hi John!");
		break;
	case "Mary":
		println("Hi Mary!");
		break;
	default:
		println("Hi!");
		break;
}
