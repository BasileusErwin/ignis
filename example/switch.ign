import { println } from "std:io";

let name: string = "John";

match name with
	| "John" -> println("Hi John!")
	| "Mary" -> println("Hi Mary!")
	| n -> println("Hi " + n + "!");

switch name {
	case "John":
		println("Hi John!");
		break;
	case "Mary":
		println("Hi Mary!");
		break;
	default:
		println("Hi!");
		break;
}
